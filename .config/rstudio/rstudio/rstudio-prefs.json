{
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "History",
            "Files",
            "Connections",
            "Help",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Environment",
            "Plots",
            "Packages",
            "Viewer"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true
    },
    "show_indent_guides": true,
    "editor_keybindings": "vim",
    "check_arguments_to_r_function_calls": true,
    "check_unexpected_assignment_in_function_call": true,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "syntax_color_console": true,
    "scroll_past_end_of_document": true,
    "editor_theme": "Solarized Dark",
    "default_project_location": "/data",
    "document_author": "Nick DiQuattro",
    "rmd_chunk_output_inline": false,
    "wrap_tab_navigation": false,
    "save_workspace": "never",
    "remove_history_duplicates": true,
    "reuse_sessions_for_project_links": true,
    "initial_working_directory": "~/repos",
    "fold_style": "begin-only",
    "posix_terminal_shell": "zsh",
    "cran_mirror": {
        "name": "Global (CDN)",
        "host": "RStudio",
        "url": "https://cran.rstudio.com/",
        "country": "us",
        "ok": 1,
        "secondary": ""
    },
    "pdf_previewer": "none",
    "load_workspace": false,
    "show_help_tooltip_on_idle": true,
    "busy_detection": "never"
}